			</xtce:IntegerParameterType>
			<xtce:IntegerParameterType signed="false" name="CCSDSVCIDType">
				<xtce:UnitSet/>
			</xtce:IntegerParameterType>
			<xtce:IntegerParameterType signed="false" name="CCSDSVersionType">
